{
  "title": "Launcher mode - all apps",
  "parameters": {
    "basic.to_delayed_action_delay_milliseconds": 500,
    "basic.to_if_alone_timeout_milliseconds": 800,
    "basic.to_if_held_down_threshold_milliseconds": 100
  },
  "rules": [
    {
      "description": "Launcher Mode",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "o"
          },
          "to": [
            {
              "set_variable": {
                "name": "launcher_mode",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "launcher_mode",
                "value": 0
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "o"
            }
          ]
        },
        {
          "conditions": [
            {
              "name": "launcher_mode",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "c"
          },
          "to": [
            {
              "shell_command": "open '/Applications/Visual Studio Code.app'"
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "name": "launcher_mode",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "x"
          },
          "to": [
            {
              "shell_command": "open '/Applications/iTerm.app'"
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "name": "launcher_mode",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "a"
          },
          "to": [
            {
              "shell_command": "open '/Applications/Google Chrome.app'"
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "name": "launcher_mode",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "q"
          },
          "to": [
            {
              "shell_command": "open '/System/Applications/System Preferences.app'"
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "name": "launcher_mode",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "f"
          },
          "to": [
            {
              "shell_command": "open '/Applications/Todoist.app'"
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "name": "launcher_mode",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "s"
          },
          "to": [
            {
              "shell_command": "open '/Applications/Spark.app'"
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "name": "launcher_mode",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "z"
          },
          "to": [
            {
              "shell_command": "open '/Applications/Obsidian.app'"
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "name": "launcher_mode",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "d"
          },
          "to": [
            {
              "shell_command": "open '/Applications/Drafts.app'"
            }
          ],
          "type": "basic"
        },
        {
          "conditions": [
            {
              "name": "launcher_mode",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "e"
          },
          "to": [
            {
              "shell_command": "open '/Applications/Spotify.app'"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}

{
  "configurations": {
    "Python: Run current script": {
      "adapter": "debugpy",
      "configuration": {
        "name": "Python: Current File",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "debugOptions": [],
        "stopOnEntry": true,
        "console": "integratedTerminal",
        "cwd": "${workspaceRoot}"
      },
      "breakpoints": {
        "exception": {
          "caught": "N",
          "uncaught": "N"
        }
      }
    },
    "Java Attach": {
      "default": true,
      "adapter": "java-debug-server",
      "configuration": {
        "name": "Java: Current File",
        "type": "java",
        "request": "attach",
        "program": "${file}",
        "console": "integratedTerminal",
        "cwd": "${workspaceRoot}",
        "debugOptions": [],
        "stopOnEntry": true,
        "host": "127.0.0.1",
        "port": "5005"
      },
      "breakpoints": {
        "exception": {
          "caught": "N",
          "uncaught": "N"
        }
      }
    }
  }
}
